{
  "name": "vsavm-fasteval",
  "version": "1.0.0",
  "description": "Rapid evaluation suite for VSAVM core capabilities",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:rule-learning": "jest --testPathPattern=rule-learning",
    "test:compression": "jest --testPathPattern=compression",
    "test:reasoning": "jest --testPathPattern=reasoning",
    "test:rl-prediction": "jest --testPathPattern=rl-prediction",
    "test:query-response": "jest --testPathPattern=query-response",
    "eval-all": "node src/eval-runner.js",
    "eval-watch": "nodemon src/eval-runner.js",
    "generate-report": "node src/report-generator.js",
    "benchmark": "node src/benchmark-runner.js"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "fs-extra": "^11.1.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "nodemon": "^2.0.20",
    "@types/jest": "^29.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/tests/**/*.test.js"],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  }
}
